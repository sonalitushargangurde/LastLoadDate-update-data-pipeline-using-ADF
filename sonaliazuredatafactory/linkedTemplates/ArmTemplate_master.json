{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "sonaliazuredatafactory"
		},
		"ls_destination_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_destination'"
		},
		"ls_dummy_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_dummy'"
		},
		"ls_lastloaddate_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_lastloaddate'"
		},
		"ls_lastloaddate_sqldb_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_lastloaddate_sqldb'"
		},
		"ls_sql_db_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_sql_db'"
		},
		"ls_lastloaddate_sqldb_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "azuresqluser.database.windows.net"
		},
		"ls_lastloaddate_sqldb_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "db01"
		},
		"ls_lastloaddate_sqldb_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "azuresqluser"
		},
		"ls_sql_db_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "azuresqluser.database.windows.net"
		},
		"ls_sql_db_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "db01"
		},
		"ls_sql_db_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "azuresqluser"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "sonaliazuredatafactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_destination_connectionString": {
						"value": "[parameters('ls_destination_connectionString')]"
					},
					"ls_dummy_connectionString": {
						"value": "[parameters('ls_dummy_connectionString')]"
					},
					"ls_lastloaddate_connectionString": {
						"value": "[parameters('ls_lastloaddate_connectionString')]"
					},
					"ls_lastloaddate_sqldb_password": {
						"value": "[parameters('ls_lastloaddate_sqldb_password')]"
					},
					"ls_sql_db_password": {
						"value": "[parameters('ls_sql_db_password')]"
					},
					"ls_lastloaddate_sqldb_properties_typeProperties_server": {
						"value": "[parameters('ls_lastloaddate_sqldb_properties_typeProperties_server')]"
					},
					"ls_lastloaddate_sqldb_properties_typeProperties_database": {
						"value": "[parameters('ls_lastloaddate_sqldb_properties_typeProperties_database')]"
					},
					"ls_lastloaddate_sqldb_properties_typeProperties_userName": {
						"value": "[parameters('ls_lastloaddate_sqldb_properties_typeProperties_userName')]"
					},
					"ls_sql_db_properties_typeProperties_server": {
						"value": "[parameters('ls_sql_db_properties_typeProperties_server')]"
					},
					"ls_sql_db_properties_typeProperties_database": {
						"value": "[parameters('ls_sql_db_properties_typeProperties_database')]"
					},
					"ls_sql_db_properties_typeProperties_userName": {
						"value": "[parameters('ls_sql_db_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}